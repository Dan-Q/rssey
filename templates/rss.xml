<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
  <channel>
    <title>{{title}}</title>
    <description>{{description}}</description>
    <link>{{link}}</link>
    <lastBuildDate>{{lastBuildDate}}</lastBuildDate>
    <pubDate>{{pubDate}}</pubDate>
    <ttl>{{ttl}}</ttl>

    {{#items}}
      <item>
        <title>{{title}}</title>
        <description>{{description}}</description>
        <link>{{link}}</link>
        <guid isPermaLink="false">{{guid}}</guid>
        <pubDate>{{pubDate}}</pubDate>
      </item>
    {{/items}}
  </channel>
</rss>